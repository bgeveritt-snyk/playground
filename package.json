{
  "name": "clockwork-web",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use \\x1b[36myarn install\\x1b[0m instead of \\x1b[31mnpm install\\x1b[0m');\"",
    "clean": "rm -rf app/assets/javascripts/bundles/* app/assets/stylesheets/bundles/*",
    "build": "yarn install && webpack --config ./config/webpack/webpack.bundles.js --bail",
    "rebuild": "yarn clean; yarn build",
    "prettier-watch": "onchange 'lib/milo/**/*.(js|jsx)' 'app/bundles/**/*.(js|jsx)' 'app/components/**/*.(js|jsx)' 'lib/js/**/*.(js|jsx)' -- prettier --write {{changed}}",
    "webpack-watch": "webpack --config ./config/webpack/webpack.bundles.js --watch",
    "watch": "yarn install && yarn webpack-watch",
    "lint": "eslint --ext .jsx --ext .js ./app/bundles ./app/components ./lib/milo",
    "lint-legacy": "jshint -c .jshintrc_legacy ./app/assets/javascripts",
    "clean-components": "rm -rf app/assets/components/*",
    "build-components": "webpack --config ./config/webpack/webpack.components.js",
    "rebuild-components": "yarn clean-components; yarn build-components",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "automock": false,
    "coveragePathIgnorePatterns": [
      "<rootDir>/app/assets/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app/components",
      "<rootDir>/app/assets",
      "<rootDir>/lib/js",
      "<rootDir>/lib/milo",
      "<rootDir>/lib"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "svg"
    ],
    "modulePathIgnorePatterns": [
      ".*/__mocks__/.*"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/components/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "<rootDir>/app/components/__mocks__/styleMock.js",
      "^packages(.*)$": "<rootDir>/packages$1"
    },
    "modulePaths": [
      "<rootDir>/app/assets/images"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "globalSetup": "<rootDir>/jest.global-setup.js",
    "setupFiles": [
      "<rootDir>/config/jest-setup.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@babel/polyfill": "7.7.0",
    "@datadog/browser-rum": "^2.2.0",
    "@headlessui/react": "^1.4.1",
    "autobind-decorator": "2.4.0",
    "classnames": "2.2.6",
    "clipboard": "1.7.1",
    "connected-react-router": "5",
    "dompurify": "^2.3.0",
    "downshift": "6.1.0",
    "eventemitter3": "2.0.3",
    "graphql": "^15.4.0",
    "hellosign-embedded": "1.6.1",
    "history": "4.7.2",
    "jest-junit": "8.0.0",
    "jquery": "3.3.1",
    "moment-timezone": "0.5.23",
    "ordinal-number-suffix": "0.1.2",
    "pluralize": "7.0.0",
    "prop-types": "15.7.2",
    "query-string": "5.1.1",
    "react": "16.9.0",
    "react-card-flip": "1.0.10",
    "react-confetti": "1.1.5",
    "react-datetime": "2.16.3",
    "react-dom": "16.9.0",
    "react-fileupload-progress": "0.4.2",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-transition-group": "2.5.0",
    "react-waypoint": "^10.1.0",
    "redux": "3.7.2",
    "redux-logger": "2.10.2",
    "redux-mock-store": "1.5.3",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "shortid": "2.2.14",
    "underscore": "1.9.1",
    "underscore.string": "3.3.5",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@storybook/addon-a11y": "6.1.9",
    "@storybook/addon-actions": "6.1.9",
    "@storybook/addon-docs": "6.1.9",
    "@storybook/addon-knobs": "6.1.9",
    "@storybook/react": "6.1.9",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/react-hooks": "3.2.1",
    "autoprefixer": "7.2.6",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-polyfill": "6.26.0",
    "concurrently": "5.1.0",
    "css-loader": "0.28.11",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "2.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "glob": "^7.1.7",
    "jest": "24.9.0",
    "jest-fetch-mock": "3.0.1",
    "jest-svg-transformer": "1.0.0",
    "jshint": "2.10.1",
    "moment-locales-webpack-plugin": "1.2.0",
    "moment-timezone-data-webpack-plugin": "1.3.0",
    "mutationobserver-shim": "0.3.7",
    "node-sass": "4.14.1",
    "onchange": "6.1.0",
    "postcss-loader": "2.1.6",
    "prettier": "1.19.1",
    "react-addons-test-utils": "15.6.2",
    "react-pdf-js-infinite": "1.2.0",
    "react-svg-loader": "3.0.1",
    "react-test-renderer": "16.13.1",
    "sass-loader": "6.0.7",
    "showdown": "1.9.1",
    "style-loader": "0.19.1",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "2.5.0",
    "url-loader": "1.1.2",
    "webpack": "3.12.0",
    "webpack-livereload-plugin": "1.2.0"
  }
}
